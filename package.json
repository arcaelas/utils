{
  "license": "ISC",
  "version": "2.0.7",
  "name": "@arcaelas/utils",
  "homepage": "https://github.com/arcaelas/utils",
  "description": "A small box of tools, which are implemented in different factions of the library.",
  "keywords": [
    "utils",
    "tools",
    "arcaelas",
    "arcaelas insiders",
    "arcaelas-insiders",
    "javascript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/arcaelas/utils.git"
  },
  "bugs": {
    "email": "community@arcaelas.com",
    "url": "https://github.com/arcaelas/utils/issues"
  },
  "main": "src/index.js",
  "files": [
    "src/**/*.js",
    "src/**/*.d.ts"
  ],
  "author": {
    "name": "Arcaelas Insiders",
    "email": "comunity@arcaelas.com",
    "url": "https://github.com/arcaelas"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "docs": "mkdocs gh-deploy --force",
    "deploy": "npm version patch && npm publish --access=public",
    "prepublishOnly": "yarn build",
    "postpublish": "find src -type f \\( -name '*.js' -o -name '*.d.ts' -o -name '*.map' \\) -delete"
  },
  "devDependencies": {
    "typescript": "^5.0.4"
  }
}
